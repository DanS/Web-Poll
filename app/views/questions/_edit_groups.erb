<div id="edit-groups" >
  <br />
  <fieldset>
    <legend>Edit group list</legend>
    <%= label_tag 'group-target', 'Enter a group name' %>
    <%= text_field_tag "group-target" %>
    <%= button_to_function 'Add Group', "insertGroupIntoAllGroupLists()"  %>
    <%= button_to_function 'Remove Group', 'removeFromAllGroupLists()' %>
    <%= button_to_function 'Done Editing', 'hideGroupEdit(1)' %>
  </fieldset>
  <br />
</div>